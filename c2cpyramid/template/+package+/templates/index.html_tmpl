<!DOCTYPE HTML>
<html lang="${lang}">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta name="Content-Language" content="${lang}" />

        <title>Camptocamp MapFish Application</title>
        
        <link rel="shortcut icon" type="image/x-icon" href="${request.static_url('{{package}}:static/app/images/favicon.ico')}" />

% if debug:
        <link rel="stylesheet" type="text/css" href="${request.static_url('{{package}}:static/lib/ext/Ext/resources/css/ext-all.css')}" />
        <link rel="stylesheet" type="text/css" href="${request.static_url('{{package}}:static/lib/ext/Ext/resources/css/xtheme-gray.css')}" />
        <link rel="stylesheet" type="text/css" href="${request.static_url('{{package}}:static/lib/openlayers/theme/default/style.css')}" />
        <link rel="stylesheet" type="text/css" href="${request.static_url('{{package}}:static/lib/geoext/resources/css/gxtheme-gray.css')}" />
        <link rel="stylesheet" type="text/css" href="${request.static_url('{{package}}:static/lib/geoext.ux/ux/Measure/resources/css/measure.css')}" />
        <link rel="stylesheet" type="text/css" href="${request.static_url('{{package}}:static/app/css/main.css')}" />
% else:
        <link rel="stylesheet" type="text/css" href="${request.static_url('{{package}}:static/build/app.css')}" />
% endif

% if debug:
        <script type="text/javascript" src="${request.static_url('{{package}}:static/lib/ext/Ext/adapter/ext/ext-base.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/lib/ext/Ext/ext-all-debug.js')}"></script>

        <!--<script type="text/javascript" src="${request.static_url('{{package}}:static/lib/proj4js/proj4js/lib/proj4js.js')}"></script>-->
        <script type="text/javascript" src="${request.static_url('{{package}}:static/lib/openlayers/lib/OpenLayers.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/lib/openlayers/lib/OpenLayers/Lang/fr.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/lib/geoext/lib/GeoExt.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/lib/geoext.ux/ux/SimplePrint/lib/GeoExt.ux/SimplePrint.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/lib/geoext.ux/ux/Measure/lib/GeoExt.ux/Measure.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/lib/geoext.ux/ux/Measure/lib/GeoExt.ux/MeasureLength.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/lib/geoext.ux/ux/Measure/lib/GeoExt.ux/MeasureArea.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/app/lib/App/Lang/en.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/app/lib/App/Lang/fr.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/app/lib/App/Map.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/app/lib/App/LayerTree.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/app/lib/App/Tools.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/app/lib/App/Print.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/app/lib/App/Locator.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/app/lib/App/Permalink.js')}"></script>
        <script type="text/javascript" src="${request.static_url('{{package}}:static/app/lib/App/main.js')}"></script>
% else:
        <script type="text/javascript" src="${request.static_url('{{package}}:static/build/app.js')}"></script>
      ## we don't build a language file for english
%     if lang != 'en':
        <script type="text/javascript" src="${request.static_url('{{package}}:static/build/lang-%s.js' % lang)}"></script>
%     endif
% endif
        <script type="text/javascript">
            <%include file="globals.js" />
            ##uncomment this line if you use the OGC proxy
            ##OpenLayers.ProxyHost = "${url(controller='ogcproxy', action='index', url='')}";
        </script>
    </head>

    <body>
        <div id="header">
            <h1>MapFish Application Header</h1>
        </div>
    </body>
</html>
